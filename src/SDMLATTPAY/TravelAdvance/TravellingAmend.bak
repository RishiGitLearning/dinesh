	<queryString><![CDATA[SELECT DISTINCT D.*,
CASE WHEN D.TSD_INFO='PREVIOUS' THEN 'Sanctioned Schedule of Tour:' 
WHEN D.TSD_INFO='CURRENT' THEN 'Sanction of revised Schedule of Tour required.' END AS TSDINFO,
CONCAT(S.TSD_DOC_NO,' ',DATE_FORMAT(S.TSD_DOC_DATE,'%d/%m/%Y')) AS OLDDOC 
FROM SDMLATTPAY.TRAVEL_SANCTION_DETAIL_AMEND D 
LEFT JOIN SDMLATTPAY.TRAVEL_SANCTION_DETAIL S 
ON LEFT(D.TSD_DOC_NO,12)=LEFT(S.TSD_DOC_NO,12)
ORDER BY TSD_INFO DESC,TSD_SR_NO;]]></queryString>

