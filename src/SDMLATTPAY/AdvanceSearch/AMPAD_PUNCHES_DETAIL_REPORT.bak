	<queryString><![CDATA[SELECT EMPID,PUNCHDATE,DAYNAME(PUNCHDATE) AS DAY,PUNCHES_NOS,ALL_PUNCHES,COALESCE(EMP_NAME,'') AS EMP_NAME FROM
(SELECT A.EMP_CODE AS EMPID,DATE1 AS PUNCHDATE,COALESCE(PUNCHES_NOS,0) AS PUNCHES_NOS,COALESCE(ALL_PUNCHES,'') AS ALL_PUNCHES FROM
(SELECT DISTINCT EMP_CODE,DATE1 FROM SDMLATTPAY.ATT_DATA,
(SELECT DISTINCT A_DATE AS DATE1 FROM SDMLATTPAY.ATT_DATA WHERE MONTH(A_DATE) =02 AND YEAR(A_DATE) = 2022) AS AC WHERE MONTH(A_DATE) =02 AND YEAR(A_DATE) = 2022
AND MACHINE=30) A
LEFT JOIN
(SELECT EMP_CODE AS EMPID,A_DATE AS PUNCHDATE,COUNT(DISTINCT P_TIME) AS PUNCHES_NOS,GROUP_CONCAT(DISTINCT P_TIME ORDER BY P_TIME ASC SEPARATOR ',    ') AS ALL_PUNCHES FROM SDMLATTPAY.ATT_DATA WHERE YEAR(A_DATE)=2022 AND MONTH(A_DATE)=2 AND MACHINE=30 
GROUP BY EMP_CODE,A_DATE) B
ON A.EMP_CODE=B.EMPID AND A.DATE1=B.PUNCHDATE) X
LEFT JOIN
( SELECT PAY_EMP_NO,EMP_NAME FROM SDMLATTPAY.ATTPAY_EMPMST ) AS EMP 
ON EMPID=PAY_EMP_NO 
ORDER BY EMPID,PUNCHDATE ;]]></queryString>

