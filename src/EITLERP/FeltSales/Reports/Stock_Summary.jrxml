	<queryString><![CDATA[SELECT PR_PRODUCT_CODE AS PRODUCT_CODE,COALESCE(PRODUCT_DESC,'') AS DESCRIPTION ,PR_GROUP AS "GROUP",COUNT(PR_PIECE_NO) AS NO_OF_PIECES,SUM(PR_LENGTH) AS LENGTH,SUM(PR_THORITICAL_WEIGHT) AS WEIGHT,SUM(PR_SQMTR) AS SQMTR,'IN STOCK' AS TYPE FROM( SELECT PR_PRODUCT_CODE,PR_GROUP,PR_PIECE_NO,PR_BILL_LENGTH+ 0E0 AS PR_LENGTH,PR_BILL_WEIGHT+ 0E0 AS PR_THORITICAL_WEIGHT,PR_BILL_SQMTR+ 0E0 AS PR_SQMTR  FROM PRODUCTION.FELT_SALES_PIECE_REGISTER WHERE PR_PIECE_STAGE ='IN STOCK' AND PR_FNSG_DATE <= '2018-02-08' UNION ALL SELECT PR_PRODUCT_CODE,PR_GROUP,PR_PIECE_NO,PR_BILL_LENGTH AS PR_LENGTH,PR_BILL_WEIGHT AS PR_THORITICAL_WEIGHT,PR_BILL_SQMTR AS PR_SQMTR  FROM PRODUCTION.FELT_SALES_PIECE_REGISTER WHERE PR_PIECE_STAGE ='IN STOCK' AND PR_FNSG_DATE <= '2018-02-08' AND PR_PACKED_DATE > '2018-02-08') AS A LEFT JOIN PRODUCTION.FELT_QLT_RATE_MASTER ON PR_PRODUCT_CODE=PRODUCT_CODE  WHERE  EFFECTIVE_FROM<=CURDATE() AND (EFFECTIVE_TO>=CURDATE() OR EFFECTIVE_TO='0000-00-00') GROUP BY  PR_PRODUCT_CODE ORDER BY PRODUCT_CODE]]></queryString>

