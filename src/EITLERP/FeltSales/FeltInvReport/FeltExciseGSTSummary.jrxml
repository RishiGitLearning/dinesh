	<queryString><![CDATA[SELECT 1 AS INV,INVOICE_NO,INVOICE_DATE,GATEPASS_NO,BALE_NO,SQMTR, 
ROUND((LENGTH*NO_OF_PIECES),2) AS LENGTH,ACTUAL_WEIGHT AS WEIGHT,(BAS_AMT-DISC_AMT+CHEM_TRT_CHG+SEAM_CHG+INSURANCE_AMT) AS DISC_BAS_AMT,
CGST_AMT,SGST_AMT,IGST_AMT,TCS_AMT,
CASE WHEN CANCELLED=1 THEN 'C' ELSE '' END AS CFLAG 
FROM PRODUCTION.FELT_SAL_INVOICE_HEADER 
WHERE (APPROVED!=0 OR CANCELLED!=0) 
#AND INVOICE_DATE='" + EITLERPGLOBAL.formatDateDB(txtinvdt.getText().trim()) + "' 
#AND LOT_NO='" + EITLERPGLOBAL.getComboCode(cmbLot) + "' 
UNION ALL 
SELECT '','T O T A L','',COUNT(DISTINCT INVOICE_NO),'',SUM(SQMTR) AS TSQMTR,
ROUND(SUM(LENGTH*NO_OF_PIECES),2) AS TLENGTH,SUM(ACTUAL_WEIGHT) AS TWEIGHT,SUM(BAS_AMT-DISC_AMT+CHEM_TRT_CHG+SEAM_CHG+INSURANCE_AMT) AS TDISCAMT,
SUM(CGST_AMT) AS TCGST,SUM(SGST_AMT) AS TSGST,SUM(IGST_AMT) AS TIGST,SUM(TCS_AMT) AS TTCS,'' 
FROM PRODUCTION.FELT_SAL_INVOICE_HEADER 
WHERE CANCELLED=0 AND APPROVED=1 
#AND INVOICE_DATE='" + EITLERPGLOBAL.formatDateDB(txtinvdt.getText().trim()) + "' 
#AND LOT_NO='" + EITLERPGLOBAL.getComboCode(cmbLot) + "']]></queryString>

