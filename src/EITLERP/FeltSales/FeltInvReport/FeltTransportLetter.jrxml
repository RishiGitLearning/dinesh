	<queryString><![CDATA[SELECT INVOICE_NO,INVOICE_DATE,TRANSPORTER_NAME,TRANSPORTER_CODE,PARTY_CODE,PARTY_NAME,STATION_CODE,TR_LET_NO,
NET_AMOUNT,INV.BALE_NO,PACKING_DATE,COALESCE(BOX_SIZE,'') AS BOX_SIZE,
COALESCE(GROSS_WEIGHT,0) AS GROSS_WEIGHT,COALESCE(CART_RATE,0) AS CART_RATE FROM
((
SELECT RIGHT(INVOICE_NO,4) AS INVOICE_NO,DATE_FORMAT(INVOICE_DATE,'%d/%m/%Y') AS INVOICE_DATE,
TRANSPORTER_NAME,TRANSPORTER_CODE,PARTY_CODE,PARTY_NAME,DISPATCH_STATION AS STATION_CODE,
INVOICE_AMT AS NET_AMOUNT,BALE_NO,PACKING_DATE,TR_LET_NO 
FROM PRODUCTION.FELT_SAL_INVOICE_HEADER 
WHERE APPROVED=1 AND CANCELLED=0 
#AND INVOICE_DATE>='2017-12-17'
) AS INV
LEFT JOIN 
(
SELECT BALE_NO,BALE_DATE,BOX_SIZE,GROSS_WEIGHT,CART_RATE 
FROM PRODUCTION.FELT_TRANSPORTER_WEIGHT 
WHERE APPROVED=1 AND CANCELED=0
) AS TRNS
ON INV.BALE_NO = TRNS.BALE_NO AND INV.PACKING_DATE = TRNS.BALE_DATE
)]]></queryString>

