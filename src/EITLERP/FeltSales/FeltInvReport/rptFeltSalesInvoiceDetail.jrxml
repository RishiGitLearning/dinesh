	<queryString><![CDATA[SELECT  A1.INVOICE_NO,A1.INVOICE_DATE,A1.PARTY_CODE,A1.PARTY_NAME,A1.INVOICE_AMT,
A1.CHARGE_CODE,A1.DUE_DAYS,VOUCHER,B1.INVOICE_AMOUNT FROM 
(SELECT  A.INVOICE_NO,SUBSTRING(A.INVOICE_DATE,1,10) AS INVOICE_DATE,A.PARTY_CODE,B.PARTY_NAME,A.INVOICE_AMT,A.CHARGE_CODE
, COALESCE(DATEDIFF(A.DUE_DATE,SUBSTRING(A.INVOICE_DATE,1,10)),0) AS DUE_DAYS
FROM PRODUCTION.FELT_SAL_INVOICE_HEADER A,DINESHMILLS.D_SAL_PARTY_MASTER B WHERE SUBSTRING(A.INVOICE_DATE,1,10)>=$P{INVOICE_DATE_FROM} 
AND SUBSTRING(A.INVOICE_DATE,1,10)<=$P{INVOICE_DATE_TO} AND A.INVOICE_TYPE=$P{INVOICE_TYPE} 
AND A.PARTY_CODE= B.PARTY_CODE AND A.APPROVED=1 AND A.CANCELLED=0) AS A1
LEFT JOIN
(SELECT INVOICE_NO,INVOICE_DATE,INVOICE_AMOUNT,
GROUP_CONCAT(DISTINCT D.VOUCHER_NO ORDER BY D.VOUCHER_NO SEPARATOR ', ') AS VOUCHER 
FROM FINANCE.D_FIN_VOUCHER_DETAIL_EX C,FINANCE.D_FIN_VOUCHER_HEADER D
 WHERE INVOICE_DATE>=$P{INVOICE_DATE_FROM} 
AND INVOICE_DATE<=$P{INVOICE_DATE_TO}  
AND C.VOUCHER_NO= D.VOUCHER_NO AND D.APPROVED =1 AND D.CANCELLED =0
GROUP BY INVOICE_NO,INVOICE_DATE) AS B1
ON A1.INVOICE_NO = B1.INVOICE_NO 
AND B1.INVOICE_DATE = A1.INVOICE_DATE  
ORDER BY A1.INVOICE_NO]]></queryString>

