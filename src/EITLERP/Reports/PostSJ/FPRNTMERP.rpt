<DBInfo value="jdbc:mysql://200.0.0.227:3306/DINESHMILLS"/>
<ReportQuery value="SELECT INV.PARTY_CODE,SUBSTRING(PARTY_NAME,1,17) AS PARTY_NAME,KG,MTR,NET,PER FROM (SELECT H.PARTY_CODE ,SUM(GROSS_KG) AS KG, SUM(GROSS_SQ_MTR) AS MTR ,SUM(H.NET_AMOUNT) AS NET,N,  ROUND((SUM(H.NET_AMOUNT)/N)*100,3) AS PER FROM D_SAL_INVOICE_DETAIL D ,D_SAL_INVOICE_HEADER H ,(SELECT SUM(NET_AMOUNT) AS N FROM DINESHMILLS.D_SAL_INVOICE_HEADER WHERE INVOICE_TYPE =2 AND INVOICE_DATE >='2016-04-01' AND INVOICE_DATE <='2016-07-31' AND CANCELLED =0 AND APPROVED =1 ) AS M WHERE D.INVOICE_DATE >= '2016-04-01' AND D.INVOICE_DATE <= '2016-07-31' AND D.INVOICE_TYPE =2 AND H.INVOICE_NO =D.INVOICE_NO AND H.INVOICE_DATE = D.INVOICE_DATE AND H.INVOICE_TYPE = D.INVOICE_TYPE AND H.CANCELLED =0 AND H.APPROVED = 1 GROUP BY H.PARTY_CODE)  AS INV LEFT JOIN (SELECT PARTY_CODE,CONCAT(PARTY_NAME,' , ',CITY_ID) AS PARTY_NAME  FROM DINESHMILLS.D_SAL_PARTY_MASTER WHERE MAIN_ACCOUNT_CODE =210010) AS PM ON PM.PARTY_CODE = INV.PARTY_CODE"/>
